      <SubComponents>
        <Component class="javax.swing.JTextArea" name="Jt_Cobertura_B">
          <Properties>
            <Property name="columns" type="int" value="20"/>
            <Property name="rows" type="int" value="5"/>
            <Property name="text" type="java.lang.String" value="La principal diferencia que tienen estos seguros con los seguros a terceros es que te ofrecen protecci&#xf3;n &#xa;contra robos, lunas e incendios y es bastante &#xfa;til en la mayor&#xed;a de los casos. "/>
          </Properties>
        </Component>
      </SubComponents>
    </Container>
